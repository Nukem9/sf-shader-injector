{
  "version": 6,
  "include": [
    "CMakeUserEnvVars.json"
  ],
  "configurePresets": [
    {
      "name": "config-base-vcpkg",
      "hidden": true,
      "toolchainFile": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
      "cacheVariables": {
        "VCPKG_OVERLAY_TRIPLETS": "${sourceDir}/vcpkg-triplets"
      }
    },
    {
      "name": "config-base-cmakedbg",
      "hidden": true,
      "binaryDir": "${sourceDir}/bin/${presetName}",
      "cacheVariables": {
        "VCPKG_TARGET_TRIPLET": "x64-windows-static-md",
        "CMAKE_CONFIGURATION_TYPES": "Debug",
        "CMAKE_MSVC_RUNTIME_LIBRARY": "MultiThreadedDebugDLL"
      }
    },
    {
      "name": "config-base-cmakerel",
      "hidden": true,
      "binaryDir": "${sourceDir}/bin/${presetName}",
      "cacheVariables": {
        "VCPKG_TARGET_TRIPLET": "x64-windows-static-md-release",
        "CMAKE_CONFIGURATION_TYPES": "Release",
        "CMAKE_MSVC_RUNTIME_LIBRARY": "MultiThreadedDLL",
        "CMAKE_INTERPROCEDURAL_OPTIMIZATION": "TRUE"
      }
    },
    {
      "name": "config-base-msvc2022",
      "hidden": true,
      "generator": "Visual Studio 17 2022",
      "architecture": {
        "value": "x64"
      },
      "cacheVariables": {
        "CMAKE_CXX_FLAGS": "/MP /diagnostics:caret"
      }
    },
    {
      "name": "config-base-ninja",
      "hidden": true,
      "generator": "Ninja Multi-Config",
      "architecture": {
        "value": "x64",
        "strategy": "external"
      }
    },
    {
      "name": "sfse",
      "hidden": true,
      "cacheVariables": {
        "BUILD_FOR_SFSE": "TRUE",
        "BUILD_FOR_ASILOADER": "FALSE"
      },
      "inherits": [
        "config-base-vcpkg",
        "config-base-ninja",
        "config-envvars"
      ]
    },
    {
      "name": "asiloader",
      "hidden": true,
      "cacheVariables": {
        "BUILD_FOR_SFSE": "FALSE",
        "BUILD_FOR_ASILOADER": "TRUE"
      },
      "inherits": [
        "config-base-vcpkg",
        "config-base-ninja",
        "config-envvars"
      ]
    },
    {
      "name": "sfse-rel",
      "displayName": "SFSE Release",
      "inherits": [
        "config-base-cmakerel",
        "sfse"
      ]
    },
    {
      "name": "sfse-dbg",
      "displayName": "SFSE Debug",
      "inherits": [
        "config-base-cmakedbg",
        "sfse"
      ]
    },
    {
      "name": "asi-rel",
      "displayName": "ASI Release",
      "inherits": [
        "config-base-cmakerel",
        "asiloader"
      ]
    },
    {
      "name": "asi-dbg",
      "displayName": "ASI Debug",
      "inherits": [
        "config-base-cmakedbg",
        "sfse"
      ]
    }
  ]
}
